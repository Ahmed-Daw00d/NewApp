@model IEnumerable<Item>
@{
    ViewData["Title"] = "Index";
}

<h1 class="text-center">Items View</h1>

<table class="table table-bordered  table-striped" style="width:100%">
    <thead>
     <tr>
        <th>
         Item Name
        </th>
        <th>
         Item Price
        </th>
         <th>
          Date
         </th>
     </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {

            <tr>
                <td>@item.Name</td>
                <td>@item.Price</td>
                <td>@item.CreatedDate</td>
            </tr>
            
        }
    </tbody>
</table>

